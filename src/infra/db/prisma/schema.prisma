generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Account {
  id         String   @id
  name       String?
  email      String   @unique
  password   String
  created_at DateTime @default(now())
  updated_at DateTime @default(now()) @updatedAt
}
